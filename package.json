{
  "name": "vue-cli",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "lidy",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --inline --progress --colors --config build/webpack.dev.conf.js",
    "start_all": "run-p start_dev start mock",
    "build": "run-s build_vue build_old",
    "build_vue": "node build/build.js",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "start_build": "node build/build.js",
    "mock": "node ./api/server.js",
    "start_dev": "run-s build_dll_dev start_server",
    "start_server": "cross-env  NODE_ENV=development webpack-dev-server --mode development --inline --host 0.0.0.0 --devtool source-maps --progress --colors --content-base .",
    "build_dll_dev": "webpack --mode development --colors --config ./webpack.dll.config.js",
    "build_dll_prod": "webpack --mode production --colors --config ./webpack.dll.config.js",
    "prod": "cross-env  NODE_ENV=production webpack --mode production --progress --colors --display-error-details",
    "build_old": "run-s build_dll_prod prod",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true npm run build_vue"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dayjs": "^1.8.11",
